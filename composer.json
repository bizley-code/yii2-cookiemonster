{
    "name": "bizley/cookiemonster",
    "type": "yii2-extension",
    "description": "Yii extension to manage cookie warning.",
    "keywords": ["yii2", "widget", "cookiemonster", "cookie", "warning", "privacy"],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/bizley/yii2-cookiemonster",
        "issues": "https://github.com/bizley/yii2-cookiemonster/issues"
    },
    "authors": [
        {
            "name": "PaweÅ‚ Bizley Brzozowski",
            "email": "pawel@positive.codes"
        }
    ],
    "require": {
        "php": ">=5.4",
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "infection/infection": "*",
        "php": ">=7.4",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "bizley\\cookiemonster\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bizley\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "yiisoft/yii2-composer": true
        }
    }
}
